{
  "name": "@ai/contracts",
  "version": "1.0.0",
  "description": "OpenAPI contracts and generated types for AI Service",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "generate": "npm run generate:types",
