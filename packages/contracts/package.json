    "clean": "rm -rf dist src/generated"
  },
  "devDependencies": {
    "typescript": "^5.8.0"
  },
  "files": [
    "dist",
    "src/generated"
  ],
  "dependencies": {
    "openapi-fetch": "^0.9.3"
  }
}
